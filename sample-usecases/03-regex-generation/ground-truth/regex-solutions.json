{
  "metadata": {
    "description": "Ground truth regex solutions with held-out test cases",
    "note": "Additional test cases not shown to model for generalization testing"
  },
  "solutions": [
    {
      "challenge_id": "REGEX-001",
      "regex": "^\\d{3}-\\d{3}-\\d{4}$",
      "explanation": "Three digits, dash, three digits, dash, four digits",
      "held_out_positive": ["000-000-0000", "111-222-3333"],
      "held_out_negative": ["555-1234-567", "55-1234-5678"]
    },
    {
      "challenge_id": "REGEX-002",
      "regex": "^[a-zA-Z0-9]([a-zA-Z0-9._]*[a-zA-Z0-9])?$",
      "explanation": "Alphanumeric start/end, dots and underscores allowed in middle, no consecutive dots",
      "held_out_positive": ["x", "ab", "user.test.name"],
      "held_out_negative": ["..test", "test..", "."]
    },
    {
      "challenge_id": "REGEX-003",
      "regex": "^(0|[1-9]\\d*)\\.(0|[1-9]\\d*)\\.(0|[1-9]\\d*)$",
      "explanation": "Three dot-separated numbers, no leading zeros except single 0",
      "held_out_positive": ["0.0.0", "999.999.999"],
      "held_out_negative": ["1.2.3.4", "01.2.3"]
    },
    {
      "challenge_id": "REGEX-004",
      "regex": "^#[0-9A-Fa-f]{6}$",
      "explanation": "Hash followed by exactly 6 hexadecimal characters",
      "held_out_positive": ["#abcdef", "#ABCDEF", "#a1B2c3"],
      "held_out_negative": ["#abc", "#abcdeff", "abcdef"]
    },
    {
      "challenge_id": "REGEX-005",
      "regex": "^((25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\.){3}(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)$",
      "explanation": "Four octets 0-255 with proper validation, no leading zeros",
      "held_out_positive": ["127.0.0.1", "8.8.8.8"],
      "held_out_negative": ["300.1.1.1", "1.1.1.01"]
    },
    {
      "challenge_id": "REGEX-006",
      "regex": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[!@#$%^&*]).{8,}$",
      "explanation": "Min 8 chars with at least one of each: lowercase, uppercase, digit, special",
      "held_out_positive": ["Xy9!abcd", "TestPass1!"],
      "held_out_negative": ["short1!", "NoSpecial1", "nouppercas1!"]
    },
    {
      "challenge_id": "REGEX-007",
      "regex": "^\\d{4}-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01])$",
      "explanation": "YYYY-MM-DD with valid month 01-12 and day 01-31",
      "held_out_positive": ["2030-07-15", "1990-01-01"],
      "held_out_negative": ["2024-02-30", "2024-00-01"]
    },
    {
      "challenge_id": "REGEX-008",
      "regex": "^[a-zA-Z_][a-zA-Z0-9_]*$",
      "explanation": "Starts with letter or underscore, followed by alphanumeric or underscore",
      "held_out_positive": ["_test123", "CamelCase"],
      "held_out_negative": ["123abc", "-test"]
    },
    {
      "challenge_id": "REGEX-009",
      "regex": "^\\*{4}-\\*{4}-\\*{4}-\\d{4}$",
      "explanation": "Three groups of 4 asterisks, then 4 digits, dash separated",
      "held_out_positive": ["****-****-****-4321"],
      "held_out_negative": ["****-****-****-", "1234-****-****-5678"]
    },
    {
      "challenge_id": "REGEX-010",
      "regex": "^([01]\\d|2[0-3]):[0-5]\\d$",
      "explanation": "Hours 00-23, colon, minutes 00-59",
      "held_out_positive": ["01:01", "13:37"],
      "held_out_negative": ["1:30", "12:60"]
    }
  ]
}
